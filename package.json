{
  "name": "@swerr/converter",
  "version": "0.0.2",
  "description": "Bundle of converters to transform Swerr source files into different documentation formats.",
  "type": "module",
  "main": "dist/index.js",
  "repository": "https://github.com/Shukaaa/swerr-converter",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch"
  },
  "keywords": [
    "swerr",
    "errors",
    "documentation",
    "api",
    "jsdoc",
    "jsdocs",
    "docs",
    "converter",
    "converters",
    "html",
    "markdown"
  ],
  "author": "Shukaaa",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.3.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@swerr/core": "^0.0.4"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
